{
  "name": "jsxlate",
  "version": "0.1.5",
  "description": "Automatic message extraction and translation for JSX files",
  "main": "index.js",
  "bin": {
    "extract-messages": "./bin/extract-messages.js",
    "translate": "./bin/translate.js",
    "transform": "./bin/transform.js",
    "bundle-messages": "./bin/bundle-messages.js"
  },
  "scripts": {
    "test": "babel-node node_modules/.bin/nodeunit tests.js",
    "build": "mkdir -p ./lib && babel --optional runtime ./jsxlate.js > lib/jsxlate.js && babel ./components.jsx > lib/components.js",
    "prepublish": "mkdir -p ./lib && babel --optional runtime ./jsxlate.js > lib/jsxlate.js && babel ./components.jsx > lib/components.js"
  },
  "author": "Eric O'Connell <eric.oconnell@idealist.org>",
  "contributors": [
    "Carl Bauer <carl@idealist.org>",
    "Ben Carr <ben@idealist.org>",
    "David McCabe <davidmccabe@gmail.com>",
    "Mike Sugarbaker <mike.sugarbaker@idealist.org>"
  ],
  "license": "MIT",
  "keywords": [
    "jsx",
    "react",
    "translation",
    "i18n",
    "l10n"
  ],
  "dependencies": {
    "babel": "^5.4.4",
    "babel-runtime": "^5.4.4",
    "chalk": "^0.5.1",
    "escodegen": "drd/escodegen#wallaby",
    "acorn-jsx": "^1.0.1",
    "immutable": "^3.7.3",
    "rw": "^0.1.4"
  },
  "peerDependencies": {
    "react": "^0.13.2"
  },
  "devDependencies": {
    "babel-loader": "^5.1.3",
    "node-inspector": "^0.10.1",
    "nodeunit": "^0.9.0",
    "webpack": "^1.9.5"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/drd/jsxlate.git"
  },
  "homepage": "http://github.com/drd/jsxlate"
}
